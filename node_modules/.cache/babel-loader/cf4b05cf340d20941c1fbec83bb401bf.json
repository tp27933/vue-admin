A,CAAA,GAAA;AACA,OAFA,EAPA,CAUA;;AACA,UAAA,WAAA,GAAA;AACA,QAAA,IAAA,EAAA,QAAA,CAAA,IADA;AAEA,QAAA,MAAA,EAAA,QAAA,CAAA,MAFA;AAGA,QAAA,WAAA,EAAA,QAAA,CAAA,WAHA;AAIA,QAAA,UAAA,EAAA,QAAA,CAAA,UAJA;AAKA,QAAA,KAAA,EAAA,QAAA,CAAA,KALA;AAMA,QAAA,MAAA,EAAA,QAAA,CAAA,MANA;AAOA,QAAA,IAAA,EAAA,QAAA,CAAA,IAPA;AAQA,QAAA,IAAA,EAAA,QAAA,CAAA;AARA,OAAA,CAXA,CAqBA;;AACA,WAAA,IAAA,GAAA,IAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,MAAA,CAAA,GAAA,EAAA,QAAA,CAAA,GAAA,CAAA;AACA;;AAEA,MAAA,MAAA,CAAA,WAAA,EAAA,GAAA,CAAA,CACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,IAAA,CAAA,QAAA,CAAA;AACA,UAAA,OAAA,EAAA,MADA;AAEA,UAAA,IAAA,EAAA;AAFA,SAAA,EADA,CAKA;AACA;AACA;AACA,OATA,EAUA,KAVA,CAUA,UAAA,KAAA,EAAA;AACA,QAAA,IAAA,CAAA,QAAA,CAAA;AACA,UAAA,OAAA,EAAA,aADA;AAEA,UAAA,IAAA,EAAA;AAFA,SAAA;AAIA,OAfA;AAgBA,KA1CA;;AA4CA,QAAA,SAAA,GAAA,SAAA,SAAA,CAAA,QAAA,EAAA;AACA,MAAA,IAAA,CAAA,QAAA,CAAA,CAAA,WAAA;AACA,KAFA;;AAIA,QAAA,YAAA,GAAA,SAAA,YAAA,CAAA,IAAA,EAAA,QAAA,EAAA;AACA,MAAA,UAAA,CAAA,KAAA,GAAA,KAAA;AACA,KAFA;;AAGA,QAAA,wBAAA,GAAA,SAAA,wBAAA,CAAA,IAAA,EAAA;AACA,MAAA,cAAA,CAAA,KAAA,GAAA,IAAA,CAAA,GAAA;AACA,MAAA,aAAA,CAAA,KAAA,GAAA,IAAA;AACA,KAHA;;AAKA,QAAA,eAAA,GAAA,SAAA,eAAA,CAAA,IAAA,EAAA,QAAA,EAAA;AACA,MAAA,UAAA,CAAA,KAAA,GAAA,IAAA;AACA,KAFA;;AAIA,WAAA;AACA,MAAA,SAAA,EAAA,SADA;AAEA,MAAA,UAAA,EAAA,UAFA;AAGA,MAAA,SAAA,EAAA,SAHA;AAIA,MAAA,eAAA,EAAA,eAJA;AAKA,MAAA,UAAA,EAAA,UALA;AAMA,MAAA,YAAA,EAAA,YANA;AAOA,MAAA,wBAAA,EAAA,wBAPA;AAQA,MAAA,QAAA,EAAA,QARA;AASA,MAAA,KAAA,EAAA,KATA;AAUA,MAAA,MAAA,EAAA,MAVA;AAWA,MAAA,aAAA,EAAA,aAXA;AAYA,MAAA,cAAA,EAAA,cAZA;AAaA,MAAA,kBAAA,EAAA;AAbA,KAAA;AAeA;AA3MA,CAAA","sourcesContent":["<template>\n  <div id=\"signup-wrap\">\n    <el-form\n      :rules=\"rules\"\n      :model=\"ruleForm\"\n      ref=\"ruleForm\"\n      label-width=\"100px\"\n      id=\"allform floatL\"\n      enctype=\"multipart/form-dat\"\n    >\n      <!-- profile upload大頭貼上傳 -->\n      <div class=\"upload floatL\">\n        <el-upload\n          action=\"https://www.mocky.io/v2/5185415ba171ea3a00704eed/posts/\"\n          ref=\"upload\"\n          list-type=\"picture-card\"\n          :before-upload=\"beforeAvatarUpload\"\n          :on-preview=\"handlePictureCardPreview\"\n          :on-remove=\"handleRemove\"\n          :class=\"{ hide: hideUpload }\"\n          :on-change=\"pictureOnchange\"\n          class=\"upload_container avatar-uploader\"\n        >\n          <i class=\"el-icon-plus\"></i>\n        </el-upload>\n      </div>\n      <!-- form 表格 -->\n      <div class=\"form floatL \">\n        <template v-for=\"(item, index) in labelData\">\n          <el-form-item :label=\"item.label\" :prop=\"item.lableProp\" :key=\"index\">\n            <template v-if=\"item.lableProp === 'gender'\">\n              <el-radio-group v-model=\"ruleForm.gender\">\n                <el-radio label=\"女\"></el-radio>\n                <el-radio label=\"男\"></el-radio>\n              </el-radio-group>\n            </template>\n\n            <template v-else-if=\"item.lableProp === 'level'\">\n              <el-select v-model=\"ruleForm.level\" placeholder=\"请选择會員卡等級\">\n                <el-option label=\"銀卡 \" value=\"sliver\"></el-option>\n                <el-option label=\"黃金\" value=\"gold\"></el-option>\n                <el-option label=\"vip\" value=\"vip\"></el-option>\n              </el-select>\n            </template>\n\n            <template v-else-if=\"item.lableProp === 'note'\">\n              <el-input type=\"textarea\" v-model=\"ruleForm.note\"></el-input>\n            </template>\n\n            <template v-else-if=\"item.lableProp === 'name'\">\n              <el-input type=\"text\" v-model=\"ruleForm.name\"></el-input>\n            </template>\n            <el-input v-else v-model.number=\"ruleForm[item.lableProp]\"></el-input>\n          </el-form-item>\n        </template>\n\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">\n            立即创建\n          </el-button>\n          <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\n        </el-form-item>\n      </div>\n    </el-form>\n    <!-- zoom-in  大頭貼放大圖 -->\n    <el-dialog :visible.sync=\"dialogVisible\">\n      <img width=\"100%\" :src=\"dialogImageUrl\" alt=\"\" />\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { Signup } from '@/api/user.js';\n\nimport { reactive, ref, refs } from '@vue/composition-api';\nexport default {\n  name: 'signup',\n  setup(props, { root, refs }) {\n    /*-----------------初始化數值----------------------*/\n\n    //( 表單label數據 )\n    const labelData = reactive([\n      {\n        label: '姓名',\n        lableProp: 'name'\n      },\n      {\n        label: '性別',\n        lableProp: 'gender'\n      },\n      {\n        label: '卡號',\n        lableProp: 'cardNumber'\n      },\n      {\n        label: '電話',\n        lableProp: 'phoneNumber'\n      },\n      {\n        label: '卡別',\n        lableProp: 'level'\n      },\n      {\n        label: '儲值金',\n        lableProp: 'amount'\n      },\n      {\n        label: '備註',\n        lableProp: 'note'\n      }\n    ]);\n\n    const hideUpload = ref(false);\n    // ( 上傳縮略圖 )\n    const dialogImageUrl = ref('');\n    const dialogVisible = ref(false);\n    // ( 表單數據容器 )\n    const ruleForm = reactive({\n      name: '',\n      gender: '',\n      phoneNumber: '',\n      cardNumber: '',\n      level: '',\n      amount: '',\n      note: '',\n      type: 'signup',\n      path: ''\n    });\n\n    const rules = reactive({\n      name: [\n        {\n          required: true,\n          message: '请输入名字',\n          trigger: 'blur'\n        }\n      ],\n      gender: [\n        {\n          required: true,\n          message: '请选择性別',\n          trigger: 'change'\n        }\n      ],\n      phoneNumber: [\n        {\n          required: true,\n          message: '電話號碼不能为空'\n        },\n        {\n          type: 'number',\n          message: '號碼必须为数字值'\n        }\n      ],\n      cardNumber: [\n        {\n          required: true,\n          message: '卡號不能为空'\n        },\n        {\n          type: 'number',\n          message: '卡號必须为数字值'\n        }\n      ],\n      level: [\n        {\n          required: true,\n          message: '请选择活會員卡級別',\n          trigger: 'change'\n        }\n      ],\n      amount: [\n        { required: true, message: '请輸入金額' },\n        {\n          type: 'number',\n          message: '金額必须为数字值'\n        }\n      ]\n    });\n\n    const beforeAvatarUpload = file => {\n      const isJPG = file.type === 'image/jpeg';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n      if (!isJPG) {\n        root.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n      if (!isLt2M) {\n        root.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n      return isJPG && isLt2M;\n    };\n\n    const submitForm = formName => {\n      refs[formName].validate(valid => {\n        if (valid) {\n          signup();\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    };\n\n    const signup = () => {\n      const fileArray = refs.upload.uploadFiles;\n      // 实例化大頭貼对象\n      const pic = new FormData();\n      // 实例化FormData对象\n      var formData = new FormData(); // Currently empty\n      // 遍历圖片，将所有文件存入fd中\n      fileArray.map(element => {\n        pic.append('avatar', element.raw);\n      });\n      //獲取用戶登入資訊\n      let requestData = {\n        name: ruleForm.name,\n        gender: ruleForm.gender,\n        phoneNumber: ruleForm.phoneNumber,\n        cardNumber: ruleForm.cardNumber,\n        level: ruleForm.level,\n        amount: ruleForm.amount,\n        note: ruleForm.note,\n        type: ruleForm.type\n      };\n      // 遍历表單數據，将所有文件存入fd中\n      for (var key in ruleForm) {\n        formData.append(key, ruleForm[key]);\n      }\n\n      Signup(requestData, pic)\n        .then(response => {\n          root.$message({\n            message: '註冊成功',\n            type: 'success'\n          });\n          // root.$router.push({\n          //   name: 'Operation',\n          // })\n        })\n        .catch(error => {\n          root.$message({\n            message: '出現錯誤,請稍後再嘗試',\n            type: 'fail'\n          });\n        });\n    };\n\n    const resetForm = formName => {\n      refs[formName].resetFields();\n    };\n\n    const handleRemove = (file, fileList) => {\n      hideUpload.value = false;\n    };\n    const handlePictureCardPreview = file => {\n      dialogImageUrl.value = file.url;\n      dialogVisible.value = true;\n    };\n\n    const pictureOnchange = (file, fileList) => {\n      hideUpload.value = true;\n    };\n\n    return {\n      labelData,\n      submitForm,\n      resetForm,\n      pictureOnchange,\n      hideUpload,\n      handleRemove,\n      handlePictureCardPreview,\n      ruleForm,\n      rules,\n      Signup,\n      dialogVisible,\n      dialogImageUrl,\n      beforeAvatarUpload\n    };\n  }\n};\n</script>\n<style lang=\"scss\">\n#signup-wrap {\n  position: relative;\n}\n.upload {\n  width: 30%;\n  height: 400 px;\n}\n.upload_container {\n  width: 60%;\n  margin: 50px auto;\n}\n.form {\n  width: 60%;\n}\n\n.el-upload--picture-card {\n  .el-icon-plus {\n    line-height: 300px !important;\n  }\n}\n\n.hide .el-upload--picture-card {\n  display: none;\n}\n\n.el-upload--picture-card {\n  width: 100%;\n  height: 300px;\n}\n.el-upload-list--picture-card {\n  .el-upload-list__item {\n    width: 100% !important;\n    height: 300px !important;\n  }\n}\n.el-table .warning-row {\n  background: oldlace;\n}\n.el-table .success-row {\n  background: #f0f9eb;\n}\n\n.avatar-uploader {\n  /deep/.el-upload {\n    border: 1px dashed #d9d9d9;\n    border-radius: 6px;\n    cursor: pointer;\n    position: relative;\n    overflow: hidden;\n  }\n}\n.avatar-uploader .el-upload:hover {\n  border-color: #409eff;\n}\n.avatar-uploader-icon {\n  font-size: 28px;\n  color: #8c939d;\n  width: 178px;\n  height: 178px;\n  line-height: 178px;\n  text-align: center;\n}\n.avatar {\n  width: 178px;\n  height: 178px;\n  display: block;\n}\n</style>\n"],"sourceRoot":"src/views/member/signup"}]}