olumn\r\n          prop=\"date\"\r\n          label=\"日期\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"name\"\r\n          label=\"姓名\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"type\"\r\n          label=\"類型\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"amount\"\r\n          label=\"金額\"\r\n        ></el-table-column>\r\n\r\n      </el-table>\r\n\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { PersonalHistory } from '@/api/user.js'\r\n\r\nimport { reactive, ref, computed, watch } from '@vue/composition-api'\r\nexport default {\r\n  name: 'history',\r\n  setup (prop, { root }) {\r\n    const childrenData = reactive({\r\n      'haircut': {\r\n        label: '剪髮'\r\n      },\r\n      'color': {\r\n        label: '染髮'\r\n      },\r\n      'perm': {\r\n        label: '燙髮'\r\n      },\r\n      'treatment': {\r\n        label: '護理'\r\n      },\r\n      'others': {\r\n        label: '其他'\r\n      },\r\n\r\n    })\r\n    const dialogTableVisible = ref(false)\r\n    const table = reactive([{}])\r\n\r\n    const data = computed(() => root.$store.state.userForm\r\n\r\n    )\r\n    const sendRequest = ((index, row) => {\r\n      let userCardNumber = root.$store.state.userForm.cardNumber\r\n      let requestData = {\r\n        userCard: userCardNumber,\r\n      }\r\n      console.log(requestData);\r\n      PersonalHistory(requestData).then(res => {\r\n        console.log(res);\r\n        let tableData1 = res.data\r\n\r\n        tableData1.map((item, index) => {\r\n          root.$set(table, index, item)\r\n        })\r\n\r\n      }).catch(err => {\r\n        console.log(err);\r\n      })\r\n    })\r\n\r\n    const getRowClassName = ({ row, rowIndex }) => {\r\n      if (row.detail == 0) {\r\n        return 'row-expand-cover';\r\n      }\r\n    }\r\n\r\n\r\n\r\n\r\n    return {\r\n      childrenData,\r\n      getRowClassName,\r\n      table,\r\n      sendRequest,\r\n      data,\r\n      dialogTableVisible,\r\n    }\r\n\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" >\r\n.row-expand-cover {\r\n  .el-table__expand-icon {\r\n    visibility: hidden !important;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views/member/operation/Components/components_Tabs"}]}