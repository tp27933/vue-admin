: { Edit, Deposit, History },\r\n  setup (props, { root, refs }) {\r\n\r\n    console.log(root.$store.state.userForm);\r\n    const switchEdit = ref(null)\r\n    const activeName = ref('first')\r\n    const data = computed({\r\n      // 取值函数\r\n      get: () => {\r\n        console.log('getter')\r\n        return root.$store.state.userForm\r\n      },\r\n      // 赋值函数\r\n      set: val => { console.log(root.$store.state.userForm.disable); }\r\n    })\r\n\r\n    const tabsDisable = computed(() => {\r\n      root.$store.state.userForm.disable\r\n    })\r\n    /*--------------------------聲明函數-----------------------------*/\r\n    const handleClick = (tab, event) => {\r\n      if (tabsDisable === true) {\r\n        disable.value = true;\r\n        return\r\n      }\r\n      switch (tab.name) {\r\n        case 'edit':\r\n          refs.txt.dialogFormVisible = true;\r\n          break;\r\n        case 'depoit':\r\n          refs.switchD.dialogFormVisible = true;\r\n          break;\r\n        case 'history':\r\n          refs.switchH.dialogTableVisible = true;\r\n          break;\r\n        default:\r\n          break;\r\n\r\n      }\r\n    }\r\n\r\n    const disableSwitch = computed(() => {\r\n      return root.$store.state.userForm.disable\r\n    })\r\n    return {\r\n      disableSwitch,\r\n      data,\r\n      activeName,\r\n      handleClick,\r\n\r\n\r\n    }\r\n  },\r\n\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.tabs {\r\n  .el-tabs__item {\r\n    color: #ffff !important;\r\n  }\r\n  #tab-user {\r\n    background: #409eff;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views/member/operation/Components"}]}