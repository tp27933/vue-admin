n        width=\"20%\"\r\n      ></el-table-column>\r\n\r\n      <el-table-column prop=\"right\" width=\"40%\"></el-table-column>\r\n\r\n      <el-table-column prop=\"profile\" width=\"40%\">\r\n        <template slot-scope=\"scope\" v-if=\"scope.row.profile == 'true'\">\r\n          <el-upload\r\n            class=\"avatar-uploader\"\r\n            action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n            :show-file-list=\"false\"\r\n            :on-success=\"handleAvatarSuccess\"\r\n            :before-upload=\"beforeAvatarUpload\"\r\n          >\r\n            <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" />\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, ref } from '@vue/composition-api'\r\nexport default {\r\n  setup(props,{root}) {\r\n    const pic = ref(true)\r\n    const  imageUrl=ref( '')\r\n    const tableData = reactive([\r\n      {\r\n        left: '姓名',\r\n        right: '王小虎',\r\n        profile: 'true',\r\n      },\r\n      {\r\n        left: '性別',\r\n        right: '女',\r\n        profile: '',\r\n      },\r\n      {\r\n        left: '卡號',\r\n        right: '199850517',\r\n        profile: '',\r\n      },\r\n\r\n      {\r\n        left: '卡別',\r\n        right: '黃金',\r\n        profile: '',\r\n      },\r\n      {\r\n        left: '餘額',\r\n        right: '328',\r\n        profile: '',\r\n      },\r\n      {\r\n        left: '注意事項',\r\n        right: '',\r\n      },\r\n      {\r\n        left: '注意事項',\r\n        right: '',\r\n      },\r\n    ])\r\n    /*---------------函數----------------------*/\r\n\r\n    const arraySpanMethod = ({ row, column, rowIndex, columnIndex }) => {\r\n      if (columnIndex === 2) {\r\n        // 合并第二行\r\n        if (rowIndex === 0) {\r\n          console.log()\r\n          // 从第二列开始\r\n          return [3, 1]\r\n          // 这里返回的是行和列的合并数量,可以返回一个数组,也可以返回一个对象,效果一样\r\n          // 这里rowspan为1是行有一行合并,colspan为3是列有3列合并,你要合并几行几列就写上相应的数字\r\n          // return {\r\n          //    rowspan: 1,\r\n          //    colspan: 3\r\n          //  }\r\n          // 这里要写一个else的判断,不然被合并列的原始数据会填充到合并之后的表格里\r\n          // 这个判断是把合并的第3列,第4列的值省略,在合并的表格右边直接填原先第5列的值,合并了几列,就省略几列的值\r\n        } else if (rowIndex === 2 || rowIndex === 1) {\r\n          return [0, 0]\r\n        }\r\n      }\r\n\r\n      if (rowIndex === 5) {\r\n        // 在这里多加一个行的判断就行\r\n        // 合并第二行\r\n        if (columnIndex === 1) {\r\n          // 从第二列开始\r\n          return [2, 2]\r\n          // 这里返回的是行和列的合并数量,可以返回一个数组,也可以返回一个对象,效果一样\r\n          // 这里rowspan为1是行有一行合并,colspan为3是列有3列合并,你要合并几行几列就写上相应的数字\r\n          // return {\r\n          //    rowspan: 1,\r\n          //    colspan: 3\r\n          //  }\r\n          // 这里要写一个else的判断,不然被合并列的原始数据会填充到合并之后的表格里\r\n          // 这个判断是把合并的第3列,第4列的值省略,在合并的表格右边直接填原先第5列的值,合并了几列,就省略几列的值\r\n        } else if (columnIndex === 2) {\r\n          return [0, 0]\r\n        }\r\n      } else if (rowIndex === 6 && (columnIndex === 1 || columnIndex === 2)) {\r\n        return [0, 0]\r\n      }\r\n\r\n      if (columnIndex === 0) {\r\n        // 合并第二行\r\n        if (rowIndex === 5) {\r\n          console.log()\r\n          // 从第二列开始\r\n          return [2, 1]\r\n        } else if (rowIndex === 6) {\r\n          return [0, 0]\r\n        }\r\n      }\r\n    }\r\n\r\n const handleAvatarSuccess=(res, file)=> {\r\n        imageUrl = URL.createObjectURL(file.raw);\r\n      }\r\n  const beforeAvatarUpload=(file)=> {\r\n        const isJPG = file.type === 'image/jpeg';\r\n        const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n        if (!isJPG) {\r\n          root.$message.error('上传头像图片只能是 JPG 格式!');\r\n        }\r\n        if (!isLt2M) {\r\n          root.$message.error('上传头像图片大小不能超过 2MB!');\r\n        }\r\n        return isJPG && isLt2M;\r\n      }\r\n\r\n\r\n\r\n\r\n\r\n    /*const tableRowClassName = ({ row, column, rowIndex, columnIndex }) => {\r\n      if (rowIndex === 5 && columnIndex === 0) {\r\n        return 'warning-row'\r\n      } else if (rowIndex === 3) {\r\n        return 'success-row'\r\n      }\r\n\r\n      return ''\r\n      //把每一行的索引放進row\r\n    }\r\n   */\r\n\r\n    return {\r\n      pic,\r\n      tableData,\r\n      arraySpanMethod,\r\n      imageUrl,\r\n      handleAvatarSuccess,\r\n      beforeAvatarUpload\r\n    }\r\n  },\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.el-table .warning-row {\r\n  background: oldlace;\r\n}\r\n.el-table .success-row {\r\n  background: #f0f9eb;\r\n}\r\n\r\n .avatar-uploader .el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n  }\r\n  .avatar-uploader .el-upload:hover {\r\n    border-color: #409EFF;\r\n  }\r\n  .avatar-uploader-icon {\r\n    font-size: 28px;\r\n    color: #8c939d;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 178px;\r\n    text-align: center;\r\n  }\r\n  .avatar {\r\n    width: 178px;\r\n    height: 178px;\r\n    display: block;\r\n  }\r\n</style>\r\n"]}]}