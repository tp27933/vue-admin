   <!-- 輸入框 -->\r\n        <el-input\r\n          v-if=\"item.type==='Input'\"\r\n          v-model=\"searchData[item.prop]\"\r\n          size=\"mini\"\r\n        ></el-input>\r\n\r\n        <!-- 日期 -->\r\n        <el-date-picker\r\n          v-if=\"item.type==='Date'\"\r\n          v-model=\"searchData[item.prop]\"\r\n        ></el-date-picker>\r\n\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport { reactive, ref, watch } from '@vue/composition-api'\r\n\r\nexport default {\r\n  name: 'search',\r\n\r\n  setup (prop, { root, emit }) {\r\n    const imgUrl = ref('')\r\n    const formInline = reactive({\r\n      user: '',\r\n      number: '',\r\n    })\r\n    const rules = reactive(\r\n      { required: true, message: '欄位不能为空' },\r\n      { type: 'number', message: '必須為電話或姓名' }\r\n    )\r\n\r\n    /*---------------函數聲明----------------*/\r\n\r\n    const checkUser = () => {\r\n      Search(formInline.user).then(SearchResponse => {\r\n        if (SearchResponse.data === '') {\r\n          root.$message.error('查無此用戶，請再次輸入')\r\n          return\r\n        }\r\n\r\n        GETPROFILE(formInline.user)\r\n          .then(response => {\r\n            if (response.data === '') {\r\n              root.$message.error('查無此用戶，請再次輸入')\r\n              return\r\n            } else if (response.data.byteLength === 0) {\r\n              SearchResponse.data.pic = 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\r\n            } else {\r\n\r\n              let img = 'data:image/png;base64,' + btoa(\r\n                new Uint8Array(response.data).reduce((data, byte) => data + String.fromCharCode(byte), '')\r\n              )\r\n              SearchResponse.data.pic = img\r\n              emit('update:formData', SearchResponse.data)\r\n\r\n              console.log('send');\r\n            }\r\n\r\n            root.$store.commit('RENDER_USER', SearchResponse.data)\r\n            root.$store.commit('SET_TABS_DISABLED', false)\r\n          })\r\n\r\n\r\n\r\n\r\n      })\r\n\r\n\r\n    }\r\n    return {\r\n      imgUrl,\r\n      formInline,\r\n      checkUser,\r\n      rules,\r\n      Search,\r\n    }\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.search {\r\n  width: 100%;\r\n  height: 42px;\r\n  margin-bottom: 15px;\r\n}\r\n</style>\r\n"]}]}