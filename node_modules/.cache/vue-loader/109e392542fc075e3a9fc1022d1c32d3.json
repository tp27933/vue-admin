    \r\n      </el-table-column>\r\n    \r\n\r\n \r\n  <el-table-column\r\n    prop=\"profile\"\r\n\r\n    width=\"180\">\r\n \r\n      <template slot-scope=\"scope\" v-if=\"scope.row.profile=='true'\">\r\n        <input name=\"file\" type=\"file\" accept=\"image/png,image/gif,image/jpeg\" @change=\"update\"/>\r\n          <el-avatar > user </el-avatar>\r\n      \r\n      </template>\r\n \r\n  </el-table-column>\r\n \r\n\r\n     \r\n    </el-table>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive ,ref} from '@vue/composition-api'\r\nexport default {\r\n  setup(){\r\n    const pic=ref(true);\r\n  \r\n     const tableData=reactive( [\r\n       { \r\n           left: '姓名',\r\n          right: '王小虎',\r\n          profile:'true'\r\n    \r\n        },\r\n         { \r\n           left: '性別',\r\n          right: '女',\r\n              profile:''\r\n    \r\n        },\r\n\r\n        { \r\n           left: '卡別',\r\n          right: '黃金',\r\n              profile:''\r\n   \r\n    \r\n        },\r\n         { \r\n           left: '餘額',\r\n          right: '328',\r\n              profile:''\r\n      \r\n    \r\n        },\r\n        { \r\n           left: '注意事項',\r\n          right: '',\r\n\r\n    \r\n        },\r\n        ] )\r\n/*---------------函數----------------------*/\r\n\r\n\r\nconst arraySpanMethod=({ row, column, rowIndex, columnIndex })=> {\r\n\r\n\r\n    if (columnIndex === 2) {\r\n      \r\n        // 合并第二行\r\n          if ( rowIndex === 0) {\r\n\r\n          console.log();\r\n          \r\n          // 从第二列开始\r\n          return [3,1]\r\n          // 这里返回的是行和列的合并数量,可以返回一个数组,也可以返回一个对象,效果一样\r\n          // 这里rowspan为1是行有一行合并,colspan为3是列有3列合并,你要合并几行几列就写上相应的数字\r\n          // return {\r\n          //    rowspan: 1,\r\n          //    colspan: 3\r\n          //  }\r\n          // 这里要写一个else的判断,不然被合并列的原始数据会填充到合并之后的表格里\r\n          // 这个判断是把合并的第3列,第4列的值省略,在合并的表格右边直接填原先第5列的值,合并了几列,就省略几列的值\r\n        } else if (rowIndex === 2 || rowIndex === 1  ) {\r\n          \r\n          return [0, 0]\r\n        }\r\n      }\r\n      \r\n    if (rowIndex === 4 ) { // 在这里多加一个行的判断就行\r\n        // 合并第二行\r\n        if (columnIndex === 1) {\r\n          // 从第二列开始\r\n          return [1, 2]\r\n          // 这里返回的是行和列的合并数量,可以返回一个数组,也可以返回一个对象,效果一样\r\n          // 这里rowspan为1是行有一行合并,colspan为3是列有3列合并,你要合并几行几列就写上相应的数字\r\n          // return {\r\n          //    rowspan: 1,\r\n          //    colspan: 3\r\n          //  }\r\n          // 这里要写一个else的判断,不然被合并列的原始数据会填充到合并之后的表格里\r\n          // 这个判断是把合并的第3列,第4列的值省略,在合并的表格右边直接填原先第5列的值,合并了几列,就省略几列的值\r\n        } else if (columnIndex === 2 ) {\r\n          return [0, 0]\r\n        }\r\n      }\r\n\r\n\r\n\r\n}\r\n\r\n    return{\r\n      pic,\r\n      tableData,\r\n      arraySpanMethod\r\n    \r\n   }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n\r\n \r\n</style>"]}]}