         {{ item.txt }}\r\n        </li>\r\n      </ul>\r\n      <!---表單 start-->\r\n      <el-form\r\n        :model=\"ruleForm\"\r\n        status-icon\r\n        :rules=\"rules\"\r\n        ref=\"ruleForm\"\r\n        class=\"login_form\"\r\n        size=\"medium\"\r\n      >\r\n        <el-form-item prop=\"uesername\" class=\"item_form\">\r\n          <label>郵箱</label>\r\n          <el-input\r\n            type=\"text\"\r\n            v-model=\"ruleForm.uesername\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"paw\" class=\"item_form\" minlength=\"6\" maxlength=\"20\">\r\n          <label>密碼</label>\r\n          <el-input\r\n            type=\"password\"\r\n            v-model=\"ruleForm.paw\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"code\" class=\"item_form\">\r\n          <label>驗證碼</label>\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"15\">\r\n              <el-input v-model.number=\"ruleForm.code\"  minlength=\"6\" maxlength=\"20\"></el-input>\r\n            </el-col>\r\n            <el-col :span=\"9\">\r\n              <el-button type=\"success\" class=\"block\">獲取驗證碼</el-button>\r\n            </el-col>\r\n          </el-row>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button\r\n            type=\"danger\"\r\n            class=\"margin_top block\"\r\n            @click=\"submitForm('ruleForm')\"\r\n          >\r\n            Submit\r\n          </el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: 'login',\r\n  data() {\r\n    //表單數據開始\r\n    var validatecode = (rule, value, callback) => {\r\n        let reg=/^[a-z0-9]{6}$/\r\n      if (value === '') {\r\n        return callback(new Error('請輸入驗證碼'))\r\n      }else if(!reg.test(value)){\r\n           return callback(new Error('驗證碼格式有誤'))\r\n      }else{\r\n          callback();\r\n      }\r\n     \r\n    }\r\n    //驗證用戶名\r\n    var validateUersname = (rule, value, callback) => {\r\n         let reg=/^\\w+([-+.]\\w+)*@\\w+([-.]\\w+)*\\.\\w+([-.]\\w+)*$/;\r\n      if (value === '') {\r\n        callback(new Error('請輸入用戶  '))\r\n      } else if (!reg.test(value)) {\r\n        callback(new Error('用戶格式有誤'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    //驗證密碼\r\n    var validatePassword = (rule, value, callback) => {\r\n        let reg=/^(?!\\D+$)(?![^a-zA-Z]+$)\\S{6,20}$/;\r\n      if (value === '') {\r\n        callback(new Error('請輸入密碼'))\r\n      } else if (!reg.test(value)) {\r\n        callback(new Error('密碼為6至20數字和字母'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    //<---表單--->\r\n    return {\r\n      menuTab: [{ txt: '登入' }, { txt: '註冊' }],\r\n      selected: 0,\r\n      ruleForm: {\r\n        uesername: '',\r\n        paw: '',\r\n        code: '',\r\n      },\r\n      rules: {\r\n        uesername: [{ validator: validateUersname, trigger: 'blur' }],\r\n        paw: [{ validator: validatePassword, trigger: 'blur' }],\r\n        code: [{ validator: validatecode, trigger: 'blur' }],\r\n      },\r\n      //表單數據結束\r\n    }\r\n  },\r\n  methods: {\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          alert('submit!')\r\n        } else {\r\n          console.log('error submit!!')\r\n          return false\r\n        }\r\n      })\r\n    },\r\n  },\r\n  created() {},\r\n  mounted() {},\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n#login {\r\n  height: 100vh;\r\n  background-color: #344a5f;\r\n}\r\n.login_wrap {\r\n  width: 330px;\r\n  margin: auto;\r\n}\r\n.menu-tab {\r\n  text-align: center;\r\n  li {\r\n    display: inline-block;\r\n    width: 88px;\r\n    height: 36px;\r\n    line-height: 36px;\r\n    font-size: 14px;\r\n    color: #ffff;\r\n    border-radius: 2px;\r\n    cursor: pointer;\r\n  }\r\n}\r\n.current {\r\n  background-color: rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n//表單 :start\r\n\r\n.login_form {\r\n  margin-top: 29px;\r\n  label {\r\n    display: block;\r\n    margin-bottom: 3px;\r\n    font-size: 14px;\r\n    color: #ffff;\r\n  }\r\n}\r\n.item_form {\r\n  margin-bottom: 13px;\r\n}\r\n.block {\r\n  width: 100%;\r\n  display: block;\r\n}\r\n.margin_top {\r\n  margin-top: 25px;\r\n}\r\n</style>\r\n"]}]}