tem label=\"审批人\">\r\n        <el-input\r\n          v-model=\"formInline.user\"\r\n          placeholder=\"审批人\"\r\n        ></el-input>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button\r\n          type=\"primary\"\r\n          @click=\"checkUser\"\r\n        >查询</el-button>\r\n\r\n      </el-form-item>\r\n    </el-form>\r\n    <img\r\n      style=\"width:200px heigh:300px\"\r\n      :src='imgUrl'\r\n      alt=\"\"\r\n    >\r\n  </div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nimport { Search, GETPROFILE } from '@/api/user.js'\r\n\r\nimport { reactive, ref, watch } from '@vue/composition-api'\r\n\r\nexport default {\r\n  name: 'search',\r\n  props: {\r\n    formData: {\r\n      tpye: Object,\r\n      default: {},\r\n    }\r\n  },\r\n  model: {\r\n    prop: 'formData',//這個字段，是指父組件設置 v-model 時，將變量值傳給子組件的 msg\r\n    event: 'parent-event'//這個字段，是指父組件監聽 parent-event 事件\r\n  },\r\n  setup (prop, { root, emit }) {\r\n    const imgUrl = ref('')\r\n    const formInline = reactive({\r\n      user: '',\r\n      number: '',\r\n    })\r\n    const rules = reactive(\r\n      { required: true, message: '欄位不能为空' },\r\n      { type: 'number', message: '必須為電話或姓名' }\r\n    )\r\n\r\n    /*---------------函數聲明----------------*/\r\n\r\n    const checkUser = () => {\r\n      Search(formInline.user).then(SearchResponse => {\r\n        if (SearchResponse.data === '') {\r\n          root.$message.error('查無此用戶，請再次輸入')\r\n          return\r\n        }\r\n\r\n        GETPROFILE(formInline.user)\r\n          .then(response => {\r\n            if (response.data === '') {\r\n              root.$message.error('查無此用戶，請再次輸入')\r\n              return\r\n            } else if (response.data.byteLength === 0) {\r\n              SearchResponse.data.pic = 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\r\n            } else {\r\n\r\n              let img = 'data:image/png;base64,' + btoa(\r\n                new Uint8Array(response.data).reduce((data, byte) => data + String.fromCharCode(byte), '')\r\n              )\r\n              SearchResponse.data.pic = img\r\n\r\n              emit('parent-event', SearchResponse.data)\r\n              console.log('send');\r\n            }\r\n\r\n            root.$store.commit('RENDER_USER', SearchResponse.data)\r\n            root.$store.commit('SET_TABS_DISABLED', false)\r\n          })\r\n\r\n\r\n\r\n\r\n      })\r\n\r\n\r\n    }\r\n    return {\r\n      imgUrl,\r\n      formInline,\r\n      checkUser,\r\n      rules,\r\n      Search,\r\n    }\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.search {\r\n  width: 100%;\r\n  height: 42px;\r\n  margin-bottom: 15px;\r\n}\r\n</style>\r\n"]}]}