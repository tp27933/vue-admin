isible\"\r\n      @open=\"sendRequest()\"\r\n    >\r\n\r\n      <el-table\r\n        :data=\"table\"\r\n        style=\"width: 100%;margin-bottom: 20px;\"\r\n        row-key=\"_id\"\r\n        border\r\n        :row-class-name=\"getRowClassName\"\r\n      >\r\n        <el-table-column type=\"expand\">\r\n          <template slot-scope=\"props\">\r\n            <el-form\r\n              label-position=\"left\"\r\n              inline\r\n              class=\"demo-table-expand\"\r\n            >\r\n              <el-form-item label=\"商品名称\">\r\n                <span>{{ props.row.detail[0]}}</span>\r\n              </el-form-item>\r\n            </el-form>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"date\"\r\n          label=\"日期\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"name\"\r\n          label=\"姓名\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"type\"\r\n          label=\"類型\"\r\n        ></el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"amount\"\r\n          label=\"金額\"\r\n        ></el-table-column>\r\n\r\n      </el-table>\r\n\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { History } from '@/api/user.js'\r\n\r\nimport { reactive, ref, computed, watch } from '@vue/composition-api'\r\nexport default {\r\n  name: 'history',\r\n  setup (prop, { root }) {\r\n    const flip = ref(false)\r\n    const dialogTableVisible = ref(false)\r\n    const table = reactive([{}])\r\n\r\n    const data = computed({\r\n      // 取值函数\r\n      get: () => {\r\n        return root.$store.state.userForm\r\n      },\r\n    })\r\n    const sendRequest = ((index, row) => {\r\n\r\n      let userCardNumber = root.$store.state.userForm.cardNumber\r\n      let requestData = {\r\n        userCard: userCardNumber,\r\n      }\r\n      History(requestData).then(res => {\r\n\r\n        let tableData1 = res.data\r\n\r\n        tableData1.map((item, index) => {\r\n          root.$set(table, index, item)\r\n        })\r\n        console.log(table[1].detail[0].amount);\r\n      }).catch(err => {\r\n        console.log(err);\r\n      })\r\n    })\r\n\r\n    const getRowClassName = ({ row, rowIndex }) => {\r\n      console.log(row);\r\n      if (row.detail == 0) {\r\n        return 'row-expand-cover';\r\n      }\r\n    }\r\n\r\n\r\n\r\n    return {\r\n      getRowClassName\r\n      table,\r\n      sendRequest,\r\n      flip,\r\n      data,\r\n      dialogTableVisible,\r\n    }\r\n\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.row-expand-cover {\r\n  .el-table__expand-icon {\r\n    visibility: hidden !important;\r\n  }\r\n}\r\n</style>"]}]}